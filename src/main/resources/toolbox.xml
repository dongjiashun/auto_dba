<?xml version="1.0"?>
<toolbox>
    <tool>
        <key>static</key>
        <scope>application</scope>
        <class>com.autodb.ops.dms.web.velocity.StaticTool</class>
    </tool>
    <tool>
        <key>sec</key>
        <scope>application</scope>
        <class>com.autodb.ops.dms.web.velocity.SpringSecurityTool</class>
    </tool>
    <tool>
        <key>date</key>
        <scope>application</scope>
        <class>org.apache.velocity.tools.generic.DateTool</class>
        <parameter name="format" value="yyyy-MM-dd HH:mm:ss"/>
    </tool>
    <tool>
        <key>number</key>
        <scope>application</scope>
        <class>org.apache.velocity.tools.generic.NumberTool</class>
    </tool>
    <tool>
        <key>esc</key>
        <scope>application</scope>
        <class>org.apache.velocity.tools.generic.EscapeTool</class>
    </tool>
    <tool>
        <key>field</key>
        <scope>application</scope>
        <class>org.apache.velocity.tools.generic.FieldTool</class>
    </tool>
    <tool>
        <key>display</key>
        <scope>application</scope>
        <class>org.apache.velocity.tools.generic.DisplayTool</class>
    </tool>
</toolbox>